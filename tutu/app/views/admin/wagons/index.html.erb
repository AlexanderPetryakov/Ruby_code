<p id="notice"><%= notice %></p>

<h1>Список вагонов</h1>

<table>
  <thead>
    <tr>
      <th>Номер вагона</th>
      <th>Тип вагона</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wagons.each do |wagon| %>
      <%= render 'wagon', wagon: wagon %>
    <% end %>
  </tbody>
</table>

<%= link_to 'Назад к списку поездов', admin_trains_path %>

